--// Load Rayfield UI
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Hacker Hub v4.5",
   LoadingTitle = "Launching Exploiting Tools...",
   LoadingSubtitle = "Fly, abuse, nuke — your way",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "LuckAdminPanel",
      FileName = "AdminConfig"
   },
   Discord = {
      Enabled = false
   },
   KeySystem = false
})

--// Main Tab
